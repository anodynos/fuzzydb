<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.whirlwind-db</groupId>
	<artifactId>whirlwind-insight-plugin</artifactId>
	<version>1.0-SNAPSHOT</version>
	<parent>
		<groupId>com.wwm</groupId>
		<artifactId>com.wwm.aspectj.parent</artifactId>
		<version>1.0-SNAPSHOT</version>
		<!-- empty to workaround 3.0.x warning: see http://jira.codehaus.org/browse/MNG-4687 -->
		<relativePath></relativePath>
	</parent>

	<dependencies>
		<dependency>
			<groupId>com.wwm.db</groupId>
			<artifactId>com.wwm.db</artifactId>
			<version>1.0-SNAPSHOT</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.springsource.insight</groupId>
			<artifactId>insight-collection</artifactId>
			<version>1.0.0.RELEASE</version>
			<scope>provided</scope>
		</dependency>
	</dependencies>

	<profiles>
		<profile>
			<id>bootstrap</id>
			<repositories>
				<repository>
					<id>spring-milestone</id>
					<name>Spring Milestone Repository</name>
					<url>http://maven.springframework.org/milestone</url>
				</repository>
				<repository>
					<id>spring-release</id>
					<name>Spring Milestone Repository</name>
					<url>http://maven.springframework.org/release</url>
				</repository>
			</repositories>
		</profile>
	</profiles>

	<build>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>aspectj-maven-plugin</artifactId>
				<version>1.0</version>
				<configuration>
					<outxml>true</outxml>
					<aspectLibraries>
						<aspectLibrary>
							<groupId>com.springsource.insight</groupId>
							<artifactId>insight-collection</artifactId>
						</aspectLibrary>
					</aspectLibraries>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
				<executions>
					<execution>
						<goals>
							<goal>compile</goal>
							<goal>test-compile</goal>
						</goals>
					</execution>
				</executions>
				<dependencies>
					<dependency>
						<groupId>org.aspectj</groupId>
						<artifactId>aspectjrt</artifactId>
						<version>${aspectj.version}</version>
					</dependency>
					<dependency>
						<groupId>org.aspectj</groupId>
						<artifactId>aspectjtools</artifactId>
						<version>${aspectj.version}</version>
					</dependency>
				</dependencies>
			</plugin>
		</plugins>
	</build>
</project>